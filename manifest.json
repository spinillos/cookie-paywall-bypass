{
  "manifest_version": 3,
  "name": "__MSG_title__",
  "description": "__MSG_description__",
  "version": "0.0.1",
  "default_locale": "es",
  "action": {
    "default_popup": "/public/index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "/public/options.html",
  "content_scripts": [
    {
      "js": [
        "/public/scripts/checking_script.js"
      ],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage"]
}
